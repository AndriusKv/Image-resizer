{
  "name": "image-editor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-js": "browserify -d src/js/dev/main.js -t babelify --outfile src/js/main.js",
    "build-scss": "node-sass src/scss/main.scss --source-map-embed src/css/main.css & npm run autoprefixer",
    "autoprefixer": "postcss --use autoprefixer src/css/main.css -d src/css/",
    "watch-js": "nodemon -e js -w src/js/dev -x 'npm run build-js'",
    "watch-scss": "nodemon -e scss -w src/scss -x 'npm run build-scss'",
    "watch": "npm run watch-js & npm run watch-scss"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "node-sass": "^3.4.2"
  },
  "dependencies": {
    "jszip": "^2.5.0"
  }
}
